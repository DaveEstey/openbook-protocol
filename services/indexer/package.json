{
  "name": "@openbook/indexer",
  "version": "0.1.0",
  "description": "OpenBook Protocol event indexer - ingests on-chain events and builds derived views",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "migrate": "node dist/migrations/run.js",
